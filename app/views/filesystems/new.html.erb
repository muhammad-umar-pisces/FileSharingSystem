<%= form_for @file do |f| %>
	<%= f.label :File_Name %>
	<%= f.text_field :file_name %><br><br>
	<%= f.hidden_field :user_id, value: current_user.id %>
	<%= f.select :status, ["public_status","private_status"]%><br><br>
	<%= f.file_field :image %><br><br>
	<%= f.submit 'Create file', :class => "btn btn-primary" %>
<% end %>